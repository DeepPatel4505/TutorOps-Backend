{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#src/*": [
                "src/*"
            ],
            "#utils/*": [
                "src/utils/*"
            ],
            "#controllers/*": [
                "src/controllers/*"
            ],
            "#middlewares/*": [
                "src/middlewares/*"
            ],
            "#services/*": [
                "src/services/*"
            ],
            "#models/*": [
                "src/models/*"
            ],
            "#config/*": [
                "src/config/*"
            ],
            "#types/*": [
                "src/types/*"
            ],
            "#routes/*": [
                "src/routes/*"
            ],
            "#validators/*": [
                "src/validators/*"
            ],
            "#api/*": [
                "src/api/*"
            ],
            "#entities/*": [
                "src/entities/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}